{
  "name": "sql-cli",
  "author": "Muhammad Hasan Khan",
  "version": "0.3.2",
  "description": "Cross platform command line interface for SQL Server",
  "tags": [
    "sql",
    "cli"
  ],
  "keywords": [
    "node",
    "azure",
    "cli",
    "sql"
  ],
  "main": "./lib/cli.js",
  "preferGlobal": "true",
  "engines": {
    "node": ">= 0.8"
  },
  "licenses": [
    {
      "type": "Apache",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "mssql": "0.5.3",
    "underscore": "~1.4.4",
    "commander": "~1.0.4",
    "easy-table": "0.3.0",
    "underscore.string": "~2.3.3",
    "q": "~1.0.1",
    "ya-csv": "~0.9.3",
    "line-by-line": "^0.1.1",
    "mstring": "^0.1.2"
  },
  "homepage": "https://github.com/hasankhan/sql-cli",
  "repository": {
    "type": "git",
    "url": "git@github.com:hasankhan/sql-cli.git"
  },
  "bugs": {
    "url": "https://github.com/hasankhan/sql-cli/issues"
  },
  "scripts": {
